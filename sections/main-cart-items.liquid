{%- style -%}
  .section-{{ section.id }}-padding {
    padding-top: {{ section.settings.padding_top | times: 0.75 | round: 0 }}px;
    padding-bottom: {{ section.settings.padding_bottom | times: 0.75 | round: 0 }}px;
  }

  @media screen and (min-width: 750px) {
    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding_top }}px;
      padding-bottom: {{ section.settings.padding_bottom }}px;
    }
  }
{%- endstyle -%}

{%- unless settings.cart_type == 'drawer' -%}
  <script src="{{ 'cart.js' | asset_url }}" defer="defer"></script>
{%- endunless -%}

<script src="{{ 'quantity-popover.js' | asset_url }}" defer="defer"></script>

<cart-items class="gradient color-{{ section.settings.color_scheme }} isolate{% if cart == empty %} is-empty{% else %} section-{{ section.id }}-padding{% endif %}">
    <div class="title-wrapper-with-link page-width">
      <h1 class="title title--primary">{{ 'sections.cart.title' | t }}</h1>
      <a href="{{ routes.all_products_collection_url }}" class="underlined-link">
        {{- 'general.continue_shopping' | t -}}
      </a>
    </div>

    <div class="cart__warnings page-width">
      <h1 class="cart__empty-text">{{ 'sections.cart.empty' | t }}</h1>
      <a href="{{ routes.all_products_collection_url }}" class="button">
        {{ 'general.continue_shopping' | t }}
      </a>

      {%- if shop.customer_accounts_enabled and customer == null -%}
        <h2 class="cart__login-title">{{ 'sections.cart.login.title' | t }}</h2>
        <p class="cart__login-paragraph">
          {{ 'sections.cart.login.paragraph_html' | t: link: routes.account_login_url }}
        </p>
      {%- endif -%}
    </div>

    <form action="{{ routes.cart_url }}" class="cart__contents critical-hidden page-width" method="post" id="cart">
      <div class="cart__items" id="main-cart-items" data-id="{{ section.id }}">
        <div class="js-contents">
          {%- if cart != empty -%}
            <div class="custom-cart cart-items">
              <caption class="visually-hidden">
                {% comment %} {{ 'sections.cart.title' | t }} {% endcomment %}
              </caption>
              <div class="custom-cart-title">
                  <div class="caption-with-letter-spacing" colspan="2" scope="col">
                    {{ 'sections.cart.headings.product' | t }}
                  </div>
                  <div class="medium-hide large-up-hide right caption-with-letter-spacing" colspan="1" scope="col">
                    {{ 'sections.cart.headings.total' | t }}
                  </div>
                  <div
                    class="cart-items__heading--wide cart-items__heading--quantity small-hide caption-with-letter-spacing"
                    colspan="1"
                    scope="col"
                  >
                    {{ 'sections.cart.headings.quantity' | t }}
                  </div>
                  <div class="small-hide right caption-with-letter-spacing" colspan="1" scope="col">
                    {{ 'sections.cart.headings.total' | t }}
                  </div>
              </div>

              <div class="product-list">
                {%- for item in cart.items -%}
                  <div class="cart-item" id="CartItem-{{ item.index | plus: 1 }}">
                    <div class="cart-item__media">
                      {% if item.image %}
                        {% comment %} Leave empty space due to a:empty CSS display: none rule {% endcomment %}
                        <a href="{{ item.url }}" class="cart-item__link" aria-hidden="true" tabindex="-1"> </a>
                        <div class="cart-item__image-container gradient global-media-settings">
                          <img
                            src="{{ item.image | image_url: width: 300 }}"
                            class="cart-item__image"
                            alt="{{ item.image.alt | escape }}"
                            loading="lazy"
                            width="150"
                            height="{{ 150 | divided_by: item.image.aspect_ratio | ceil }}"
                          >
                        </div>
                      {% endif %}
                    </div>
                    <div class="card-wrapper custom-detail">
                      <div class="cart-item__details">
                        {%- if settings.show_vendor -%}
                          <p class="caption-with-letter-spacing">{{ item.product.vendor }}</p>
                        {%- endif -%}

                        {% comment %} <a href="{{ item.url }}" class="cart-item__name h4 break">{{ item.product.title | escape }}</a> {% endcomment %}
                        <div class="card_text card__content">
                          <div class="card__information">
                            <h3
                              class="card__heading{% if item.featured_media or settings.card_style == 'standard' %} h5{% endif %}"
                              {% if item.featured_media or settings.card_style == 'card' %}
                                id="title-{{ section_id }}-{{ item.id }}"
                              {% endif %}
                            >
                              <a
                                href="{{ item.url }}"
                                id="CardLink-{{ section_id }}-{{ item.id }}"
                                class="full-unstyled-link {% if item.product.metafields.reviews.rating.value != blank %}show-review{% endif %}"
                                aria-labelledby="CardLink-{{ section_id }}-{{ item.id }} Badge-{{ section_id }}-{{ item.id }}"
                              >
                                {{ item.title | escape | truncate: 30, ""}}
                              </a>
                            </h3>
                            <div class="c card-information">
                            
                                {% liquid
                                  assign rating_decimal = 0
                                  assign decimal = item.product.metafields.reviews.rating.value.rating | modulo: 1
                                  if decimal >= 0.3 and decimal <= 0.7
                                    assign rating_decimal = 0.5
                                  elsif decimal > 0.7
                                    assign rating_decimal = 1
                                  endif
                                %}
                                <div class="custom-rating">
                                  <div
                                    class="rating"
                                    role="img"
                                    aria-label="{{ 'accessibility.star_reviews_info' | t: rating_value: item.product.metafields.reviews.rating.value, rating_max: item.product.metafields.reviews.rating.value.scale_max }}"
                                  >
                                    <script>{{item.product.metafields.reviews.rating.value | json }}</script>
                                    <span class="rating-value">{{item.product.metafields.reviews.rating.value }}</span>
                                    <span
                                      aria-hidden="true"
                                      class="rating-star"
                                      style="--rating: 5; --rating-max: 5.0; --rating-decimal: 0;"
                                    >
                                    </span>
                                  </div>
                                  <p class="rating-count caption">
                                    {% assign min = 6.0 %}
                                    {% assign max = 10.0 %}                 
                                    {% assign diff = max | minus: min %}
                                    {% assign randomNumber = "now" | date: "%N" | modulo: diff | plus: min | divided_by: 1.1 | round: 1 %}
                                    {% comment %} {%- assign rating_value = card_product.metafields.reviews.rating_count | plus: randomNumber -%} {% endcomment %}
                                    <span aria-hidden="true">({{ randomNumber }}K)</span>
                                  


                                    <span class="visually-hidden">
                                      {{- item.product.metafields.reviews.rating_count }}
                                      {{ 'accessibility.total_reviews' | t -}}
                                    </span>
                                  </p>
                                </div>                           
                                
                                <p class="rating-text caption">
                                  <span aria-hidden="true">
                                    {{- item.product.metafields.reviews.rating.value }} /
                                    {{ item.product.metafields.reviews.rating.value.scale_max -}}
                                  </span>
                                </p>                 
                                {% assign min_number  = 200 %}
                                {% assign max_number  = 500 %}
                                {% assign diff_sold = max_number | minus: min_number %}
                                {% assign random_number = "now" | date: "%N" | modulo: diff_sold | plus: min_number %}
